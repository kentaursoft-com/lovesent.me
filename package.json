{
	"name": "lovesent",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "wrangler d1 migrations apply lovesent-db --local",
		"db:migrate:prod": "wrangler d1 migrations apply lovesent-db --remote"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/canvas-confetti": "^1.9.0",
		"drizzle-kit": "^0.31.9",
		"svelte": "^5.49.2",
		"svelte-check": "^4.3.6",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"wrangler": "^4.65.0"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.989.0",
		"@sveltejs/adapter-cloudflare": "^7.2.7",
		"@tailwindcss/vite": "^4.1.18",
		"canvas-confetti": "^1.9.4",
		"daisyui": "^5.5.18",
		"drizzle-orm": "^0.45.1",
		"html2canvas": "^1.4.1",
		"jose": "^6.1.3",
		"lucide-svelte": "^0.563.0",
		"tailwindcss": "^4.1.18"
	}
}
